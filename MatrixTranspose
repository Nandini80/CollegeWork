#include <bits/stdc++.h>
using namespace std;
#define lli long long int
int main() 
{
    int n,m;
    cout << "enter rows and cols : ";
    cin >> n >> m;
    vector<vector<int> > v(n,vector<int> (m,0));
    for(int i=0 ;i <n ; i++)
    for(int j=0 ;j <m ; j++)
    cin >> v[i][j];

    cout << "Tranpose is : " << endl;
    for(int j=0 ; j<m ;j ++)
    {
        for(int i=0 ;i <n ; i++)
        {
            cout << v[i][j] << " ";
        }
        cout << endl;
    }

}
